{
  "urls": {
    "items": "https://www.adidas.at/api/plp/content-engine/search?",
    "reviews": "https://www.adidas.at/api/models/{model_id}/reviews?bazaarVoiceLocale=de_AT&feature&includeLocales=de%2A&limit={limit}&offset={offset}&sort=newest"
  },
  "paths": {
    "settings_file_path": "prefs/settings.json",
    "product_file_name_prefix": "pr-",
    "product_files_path": "files"
  },
  "templates": {
    "params": {
      "query": "all",
      "start": 0,
      "sort": "newest-to-oldest"
    },
    "headers": {
      "authority": "www.adidas.at",
      "accept": "*/*",
      "accept-language": "en-US,en;q=0.9,fa;q=0.8",
      "content-type": "application/json",
      "user-agent": "PostmanRuntime/7.35.0"
    }
  }
}